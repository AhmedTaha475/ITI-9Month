@using MVC_Day2_Task.Models;
@{
    ViewBag.Title = "GetAllCars";

    List<car> list = ViewBag.Cars;

}



<h2 class="text-danger">List of all Cars</h2>



<table class="table">
    <thead>
        <tr>
            <th scope="col">Num</th>
            <th scope="col">Color</th>
            <th scope="col">Model</th>
            <th scope="col">Manfacture</th>
            <th scope="col">&nsub; </th>
            <th scope="col">&nsub;</th>
            <th scope="col">&nsub;</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in list)
        {
            <tr>
                <td>@item.Num</td>
                <td>@item.Color</td>
                <td>@item.Model</td>
                <td>@item.Manfacture</td>

                <td><a class="btn btn-primary" href="/Car/SelectCarByID/@item.Num"> Select </a> </td>
                <td><a class="btn btn-success" href="/Car/UpdateCar/@item.Num"> Edit </a> </td>
                <td><a class="btn btn-danger" href="/Car/DeleteCar/@item.Num"> Delete </a> </td>

            </tr>
        }

    </tbody>
</table>

<a class="btn btn-primary" href="/Car/CreateNewCar"> Create New Car</a>


